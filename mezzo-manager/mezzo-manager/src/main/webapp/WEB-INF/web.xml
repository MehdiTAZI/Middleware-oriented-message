<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>GWT-Maven-Archetype</display-name>	
	
	<!-- servlet loaded into servlet container -->
	<servlet>
  		<servlet-name>shellExecuterImpl</servlet-name>
  		<servlet-class>
  			fr.esiag.mezzodijava.mezzo.manager.server.ShellExecuterImpl
  		</servlet-class>
	</servlet>
	<servlet-mapping>
  		<servlet-name>shellExecuterImpl</servlet-name>
  		<url-pattern>/fr.esiag.mezzodijava.mezzo.manager.Application/shellExecuter</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
</web-app>

